	"require": {
		"php": ">= 8.0",
		"nette/application": "^3.0",
		"nette/bootstrap": "^3.1",
		"nette/caching": "^3.2",
		"nette/di": "^3.0",
		"nette/forms": "^3.1",
		"nette/http": "^3.2",
		"nette/security": "^3.1",
		"nette/utils": "^3.1",
		"latte/latte": "^3.0",
